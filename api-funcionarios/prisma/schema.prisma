generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Funcionario {
  id      Int    @id @default(autoincrement())
  name    String
  salario Float
  cargo   String
  idade   Int
}

model Books {
  id          Int     @id @default(autoincrement())
  name        String
  autor       String
  year        Int
  price       Float
  isAvailable Boolean
  quantidade  Int
}
